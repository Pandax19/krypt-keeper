     </video>

     <header class="title">
    <nav>
        
        <a id="header" href="/" class="title">Krypt Keepers</a>
        <ul>
            <li>
                <a href="/"class="title">Home</a>
            </li>
            <li>
                <a href="/attractions" class="title">Activities and Attractions</a>
            </li>
            {{#unless logged_in}}
            <li>
                <a href="/login" class="test">Log In</a>
            </li>
            <li>
                <a href="/signup" class="test" >Sign Up</a>
            </li>
            {{/unless}}
            {{#if logged_in}}
            <li>
                <button id="logout" class="test"> Log Out</button>
            </li>
            {{/if}}
        </ul>
    </nav>
</header>

